<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Embeded - GET response test" configfailurepolicy="continue">
    <test name="[GET /dashboard]">
        <parameter name="GetSensorsTest" value="getSensors.json"/>
        <classes>
            <class name="org.intive.patronage.services.test.GetSensorsTest"/>
        </classes>
    </test>
    <test name="[GET /Notifications]">
        <parameter name="GetNotificationsTest" value="getNotifications.json"/>
        <classes>
            <class name="org.intive.patronage.services.test.GetNotificationsTest"/>
        </classes>
    </test>
    <test name="[DELETE /Notifications/id]">
        <parameter name="DeleteNotificationsTest" value="deleteNotifications.json"/>
        <classes>
            <class name="org.intive.patronage.services.test.DeleteNotificationsTest"/>
        </classes>
    </test>
</suite>